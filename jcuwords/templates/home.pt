  <body>
    <div tal:replace="structure keyword_cloud">
        Cloud
    </div>
    <a tal:attributes="href request.route_url('keyword_add')">Add keywords</a>
    <a href="#" tal:attributes="href return_url">Back to the JCU Futures site</a>
  </body>
</html>
